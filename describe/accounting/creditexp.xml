<xml>
   <Table System="accounting" SoftDelete="1" Icon="expense" CanDelete="0" name="Credit Expenses" UserStamp="1">
      <Field Caption="Date" Size="4" Type="5" Force="1" name="date" Default="1"/>
      <Field Caption="Reference" Size="4" Type="2" AutoCounter="1" name="reference"/>
      <Field Caption="Document" File="accounting.document" Size="2" Type="2" Force="1" name="document"/>
      <Field Caption="Creditor" File="accounting.people" Size="4" Type="2" Force="1" name="people" Dll="1" FilterFunction="FilterCustomer"/>
      <Field Caption="Project" File="accounting.location" Size="2" Type="2" Force="1" name="location"/>
      <Field Caption="Gross" Total="1" Format="######.##" Size="4" Type="4" Force="1" name="gross"/>
      <Field Caption="Discount" Total="1" Format="#####.##" Size="4" Type="4" name="discount"/>
      <Field Caption="Note" Size="50" Type="1" name="note"/>
      <Field Strict="1" Total="1" Caption="VAT" Format="#####.##" Size="4" Type="4" name="vatamount" Modify="1"/>
      <Field Strict="1" Total="1" Caption="Total" Format="#####.##" Size="4" Type="4" name="total" Modify="1"/>
      <Field Strict="1" Caption="VAT code" File="accounting.vat" Size="2" Type="2" name="vat" Modify="1"/>
      <Field Strict="1" Caption="Journal" File="accounting.journal" Size="4" Type="2" name="journal" Modify="1"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Field Caption="SoftDelete" Format="b" Hide="1" System="1" Size="1" Type="2" name="@delete" HideReport="1"/>
      <Field Caption="User" Hide="1" File="system.user" System="1" Size="2" Type="2" name="@user_stamp"/>
      <Index name="creditexp">
         <IndexEntry name="date"/>
         <IndexEntry name="reference"/>
         <IndexEntry name="document"/>
         <IndexEntry name="people"/>
      </Index>
      <Index name="creditexp[document]">
         <IndexEntry name="document"/>
         <IndexEntry name="date"/>
         <IndexEntry name="reference"/>
         <IndexEntry name="people"/>
      </Index>
      <Index name="creditexp[journal]">
         <IndexEntry name="journal"/>
         <IndexEntry name="date"/>
         <IndexEntry name="reference"/>
         <IndexEntry name="document"/>
         <IndexEntry name="people"/>
      </Index>
      <Index name="creditexp[location]">
         <IndexEntry name="location"/>
         <IndexEntry name="date"/>
         <IndexEntry name="reference"/>
         <IndexEntry name="document"/>
         <IndexEntry name="people"/>
      </Index>
      <Index name="creditexp[people]">
         <IndexEntry name="people"/>
         <IndexEntry name="date"/>
         <IndexEntry name="reference"/>
         <IndexEntry name="document"/>
      </Index>
      <Index name="creditexp_reference">
         <IndexEntry name="reference"/>
         <IndexEntry name="date"/>
         <IndexEntry name="document"/>
         <IndexEntry name="people"/>
      </Index>
   </Table>
</xml>
