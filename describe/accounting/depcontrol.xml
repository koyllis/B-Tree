         <IndexEntry name="location"/>
         <IndexEntry name="duedate"/>
         <IndexEntry name="bank"/>
         <IndexEntry name="cheque"/>
      </Index>
      <Index name="depcontrol[bank]">
         <IndexEntry name="bank"/>
         <IndexEntry name="location"/>
         <IndexEntry name="duedate"/>
         <IndexEntry name="cheque"/>
      </Index>
      <Index name="depcontrol[cashduedate]">
         <IndexEntry name="cashduedate"/>
         <IndexEntry name="location"/>
         <IndexEntry name="duedate"/>
         <IndexEntry name="bank"/>
         <IndexEntry name="cheque"/>
      </Index>
      <Index name="depcontrol[people]">
         <IndexEntry name="people"/>
         <IndexEntry name="location"/>
         <IndexEntry name="duedate"/>
         <IndexEntry name="bank"/>
         <IndexEntry name="cheque"/>
      </Index>
      <Index name="depcontrol_cheque">
         <IndexEntry name="cheque"/>
         <IndexEntry name="location"/>
         <IndexEntry name="duedate"/>
         <IndexEntry name="bank"/>
      </Index>
   </Table>
</xml>
