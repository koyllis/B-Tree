      <Field Caption="Cheque No." Size="4" Type="2" Force="1" name="cheque" Strict="1"/>
      <Field Caption="Amount" Total="1" Format="#####.##" Size="4" Type="4" Force="1" name="amount" Strict="1"/>
      <Field Caption="Page" Size="1" Type="2" name="page" Modify="1" Persistent="1"/>
      <Field Caption="Pending" Format="b" Size="1" Type="2" name="pending" Modify="1" Persistent="1"/>
      <Field Caption="Customer" File="accounting.people" Size="4" Force="1" Type="2" name="people" Strict="1"/>
      <Field Strict="1" Caption="Cash due date" File="accounting.cashduedate" Size="4" Type="2" name="cashduedate" Modify="1"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Field Caption="User" Hide="1" File="system.user" System="1" Size="2" Type="2" name="@user_stamp"/>
      <Field Strict="1" Caption="Created" System="1" Size="8" Type="5" name="@create_stamp"/>
