<xml>
   <Table Bookmark="1" System="lab" Icon="lego" Color="1" name="Analysis Groups">
      <Field Size="60"/>
      <Field Caption="Price" Format="#####.##" Size="4" Type="4" name="price"/>
      <Field Caption="Barcodes" Format="##" Size="1" Type="2" name="barcode"/>
      <Field Caption="Sequence" Format="###" Size="2" Type="2" name="sequence"/>
      <Field Caption="Invoice item" File="pos.item" Size="4" Type="2" name="item"/>
      <Field Caption="Notes header" Size="20" name="header"/>
      <Field Caption="New items" File="pos.category" Size="2" Type="2" name="category"/>
      <Field Caption="Remark group" File="lab.remarkgroup" Size="2" Type="2" name="remarkgroup"/>
      <Field Caption="Default method" File="lab.method" Size="2" Type="2" name="method"/>
      <Field Caption="Layout name" Size="20" Type="1" name="layout"/>
      <Field Caption="Option" Size="1" Type="2" Format="b" Options="NoExtras,ForceExtras,RoundResult,NotAccredited" name="option"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Field Caption="Foreground Color" Hide="1" System="1" Size="4" Type="2" name="@color_front"/>
      <Field Caption="Background Color" Hide="1" System="1" Size="4" Type="2" name="@color_back"/>
      <Field Caption="Bookmark" Hide="1" System="1" Size="32" name="@bookmark"/>
      <Index name="analysisgroup">
         <IndexEntry name="name"/>
      </Index>
      <Index name="analysisgroup[category]">
         <IndexEntry name="category"/>
         <IndexEntry name="name"/>
      </Index>
      <Index name="analysisgroup[item]">
         <IndexEntry name="item"/>
         <IndexEntry name="name"/>
      </Index>
      <Index name="analysisgroup[method]">
         <IndexEntry name="method"/>
         <IndexEntry name="name"/>
      </Index>
      <Index name="analysisgroup[remarkgroup]">
         <IndexEntry name="remarkgroup"/>
         <IndexEntry name="name"/>
      </Index>
      <Index Force="1" name="analysisgroup_@bookmark">
         <IndexEntry name="@bookmark"/>
         <IndexEntry name="name"/>
      </Index>
      <Index Force="1" name="analysisgroup_sequence">
         <IndexEntry name="sequence"/>
         <IndexEntry name="name"/>
      </Index>
   </Table>
</xml>
