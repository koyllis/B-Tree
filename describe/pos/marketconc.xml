      </Index>
      <Index name="marketconc[item]">
         <IndexEntry name="item"/>
         <IndexEntry name="people"/>
         <IndexEntry name="date"/>
         <IndexEntry name="refer"/>
         <IndexEntry name="color"/>
         <IndexEntry name="refer"/>
      </Index>
   </Table>
</xml>
