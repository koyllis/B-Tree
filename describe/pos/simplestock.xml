<xml xmlns="x-schema:c:\mmhbb\describe\gen.xdr">
   <Table System="pos" CanDelete="0" Icon="stock" name="Simple Stock">
      <Field Caption="Item" File="pos.item" Size="4" Type="2" Force="1" name="item"/>
      <Field Caption="Location" File="system.location" Size="2" Type="2" Force="1" name="location"/>
      <Field Caption="Opening Pieces" Format="######" Size="4" Type="2" name="pieces_open"/>
      <Field Caption="Pieces In" Format="######" Size="4" Type="2" name="pieces_in" Total="1"/>
      <Field Caption="Pieces Out" Format="######" Size="4" Type="2" name="pieces_out" Total="1"/>
      <Field Caption="Closing Pieces" Format="######" Size="4" Type="2" name="pieces"/>
      <Field Caption="Opening Quantity" Format="quantity" Size="4" Type="8" name="quantity_open"/>
      <Field Caption="Quantity In" Format="quantity" Size="4" Type="8" name="quantity_in" Total="1"/>
      <Field Caption="Quantity Out" Format="quantity" Size="4" Type="8" name="quantity_out" Total="1"/>
      <Field Caption="Closing Quantity" Format="quantity" Size="4" Type="8" name="quantity"/>
      <Field Caption="Date" Size="4" Type="5" name="date"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Index name="simplestock">
         <IndexEntry name="item"/>
         <IndexEntry name="location"/>
      </Index>
      <Index name="simplestock[location]">
         <IndexEntry name="location"/>
         <IndexEntry name="item"/>
      </Index>
   </Table>
</xml>
