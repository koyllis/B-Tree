      <Field Caption="Fabric width" Size="20" name="fabric_width"/>
      <Field Caption="Fabric Leadband" Size="1" Type="2" Options="LeadBand,NoLeadBand" name="fabric_band"/>
      <Field Caption="Lining name" Size="40" name="lining_name"/>
      <Field Caption="Rail type/hooks" Size="40" name="rail_name"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Field Caption="SoftDelete" Format="b" Hide="1" System="1" Size="1" Type="2" name="@delete" HideReport="1"/>
      <Index name="transdetcalc">
         <IndexEntry name="transdet"/>
      </Index>

      <Index name="transdetcalc_room">
         <IndexEntry name="room"/>
         <IndexEntry name="transdet"/>
      </Index>

   </Table>
</xml>
