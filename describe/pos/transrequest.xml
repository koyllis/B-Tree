<xml>
   <Table System="pos" name="Transfer Requests">
      <Field Caption="Date" Size="4" Force="1" Type="5" name="date"/>
      <Field Caption="Location" File="system.location" Size="2" Type="2" Force="1" name="location"/>
      <Field Caption="Item" File="pos.item" Size="4" Type="2" Force="1" name="item"/>
      <Field Caption="Color" File="pos.color" Size="2" Type="2" name="color"/>
      <Field Caption="Size" File="pos.size" Size="2" Type="2" Force="1" name="size"/>
      <Field Caption="Reference No." Size="20" name="reference"/>
      <Field Caption="Quantity" Format="quantity" Size="4" Type="8" Force="1" name="quantity"/>
      <Field Caption="Transaction" Strict="1" File="pos.trans" Size="4" Type="2" name="trans" Modify="1"/>
      <Field Caption="Modified" Hide="1" System="1" Size="8" Type="5" name="@modify_stamp"/>
      <Index name="transrequest">
         <IndexEntry name="date"/>
         <IndexEntry name="location"/>
         <IndexEntry name="item"/>
         <IndexEntry name="color"/>
         <IndexEntry name="size"/>
         <IndexEntry name="reference"/>
      </Index>
      <Index name="transrequest[color]">
         <IndexEntry name="color"/>
         <IndexEntry name="date"/>
         <IndexEntry name="location"/>
         <IndexEntry name="item"/>
         <IndexEntry name="size"/>
         <IndexEntry name="reference"/>
      </Index>
      <Index name="transrequest[item]">
         <IndexEntry name="item"/>
         <IndexEntry name="date"/>
         <IndexEntry name="location"/>
         <IndexEntry name="color"/>
         <IndexEntry name="size"/>
         <IndexEntry name="reference"/>
      </Index>
      <Index name="transrequest[size]">
         <IndexEntry name="size"/>
         <IndexEntry name="date"/>
         <IndexEntry name="location"/>
         <IndexEntry name="item"/>
         <IndexEntry name="color"/>
         <IndexEntry name="reference"/>
      </Index>
      <Index name="transrequest[trans]">
         <IndexEntry name="trans"/>
         <IndexEntry name="date"/>
         <IndexEntry name="location"/>
         <IndexEntry name="item"/>
         <IndexEntry name="color"/>
         <IndexEntry name="size"/>
         <IndexEntry name="reference"/>
      </Index>
   </Table>
</xml>
